        "import cv2\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "to_convert_image = cv2.imread('Bozu.png')\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 12,
      "metadata": {
        "id": "tMW05XQWDr07"
      },
      "outputs": [],
      "source": [
        "def negative(to_convert_image):\n",
        "    img = 255 - to_convert_image\n",
        "\n",
        "    plt.title('Inverted') # title the figure\n",
        "    plt.imshow(img) # show image\n",
        "    plt.savefig('negative_bozu.png') # saving plot in a new file named negative_bozu.png\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 13,
      "metadata": {
        "id": "TCyBFbx2Dr08"
      },
      "outputs": [],
      "source": [
        "def grayify(to_convert_image):\n",
        "    img = to_convert_image\n",
        "\n",
        "    R, G, B = img[:,:,0], img[:,:,1],img[:,:,2] #  Extracting the three color channels\n",
        "    Y = 0.2989*R + 0.5870*G + 0.1140*B     # Applying Weighted Method according to the predifined function\n",
        "\n",
        "    plt.title('Gray Image') # Title the figure\n",
        "    plt.imshow(Y, cmap='gray')    # Showing image\n",
        "    plt.savefig('Gray_Bozu.jpg')  #Saving image\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 14,
      "metadata": {
        "id": "Z5koNCKaDr08"
      },
      "outputs": [],
      "source": [
        "def reddify(to_convert_image):\n",
        "    red_img = to_convert_image.copy()  # copies image, so the original one isn't altered\n",
        "    red_img[:, :, 1] = 0 # mute green channel\n",
        "    red_img[:, :, 2] = 0 # mute blue channel\n",
        "\n",
        "    plt.title('Reddified Image')\n",
        "    plt.imshow(red_img)\n",
        "    plt.savefig('reddified_bozu.png') # saving plot in a new file named reddified_bozu.png\n",
        "\n",
        "\n",
        "def greenify(to_convert_image):\n",
        "    green_img = to_convert_image.copy()  # copies image, so the original one isn't altered\n",
        "    green_img[:, :, 0] = 0  # mute red channel\n",
        "    green_img[:, :, 2] = 0  # mute blue channel\n",
        "\n",
        "    plt.title('Greenified Image')\n",
        "    plt.imshow(green_img)\n",
        "    plt.savefig('greenified_bozu.png') # saving plot in a new file named greenified_bozu.png\n",
        "\n",
        "\n",
        "def blueify(to_convert_image):\n",
        "    blue_img = to_convert_image.copy()  # copies image, so the original one isn't altered\n",
        "    blue_img[:, :, 0] = 0  # mute red channel\n",
        "    blue_img[:, :, 1] = 0  # mute green channel\n",
        "\n",
        "    plt.title('Blueified Image')\n",
        "    plt.imshow(blue_img)\n",
        "    plt.savefig('blueified_bozu.png') # saving plot in a new file named blueified_bozu.png\n",
        "\n",
        "\n",
      ]
    },
